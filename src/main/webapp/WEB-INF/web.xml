<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>rentalcar-server</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<!-- Servlet mapping -->
	<servlet>
		<servlet-name>joinFormAction</servlet-name>
		<servlet-class>rentalServer.user.model.JoinFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinFormAction</servlet-name>
		<url-pattern>/joinFormAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>loginFormAction</servlet-name>
		<servlet-class>rentalServer.user.model.LoginFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginFormAction</servlet-name>
		<url-pattern>/loginFormAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>findUserIdAction</servlet-name>
		<servlet-class>rentalServer.user.model.FindUserIdAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findUserIdAction</servlet-name>
		<url-pattern>/findUserIdAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>findUserPwAction</servlet-name>
		<servlet-class>rentalServer.user.model.FindUserPasswordAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findUserPwAction</servlet-name>
		<url-pattern>/findUserPwAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updateUserAction</servlet-name>
		<servlet-class>rentalServer.user.model.UpdateUserAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateUserAction</servlet-name>
		<url-pattern>/updateUserAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteUserAction</servlet-name>
		<servlet-class>rentalServer.user.model.DeleteUserAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteUserAction</servlet-name>
		<url-pattern>/deleteUserAction</url-pattern>
	</servlet-mapping>
	
	<!-- Jsp mapping -->
	<servlet>
		<servlet-name>header</servlet-name>
		<jsp-file>/views/module/header.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>header</servlet-name>
		<url-pattern>/header</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>footer</servlet-name>
		<jsp-file>/views/module/footer.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>footer</servlet-name>
		<url-pattern>/footer</url-pattern>
	</servlet-mapping>

	<!-- â†‘header/footer -->
	
	<servlet>
		<servlet-name>join</servlet-name>
		<jsp-file>/views/user/join.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>join</servlet-name>
		<url-pattern>/join</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/views/user/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
